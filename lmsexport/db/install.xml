        <FIELD NAME="timezone1" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="The value to match against"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
