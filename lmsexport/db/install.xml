    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_lmsexport" COMMENT="Maps profile field values onto themes">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="apiurl" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="The value to match against"/>
